(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR A B C D E F G)
(RULES
  append(A,B) -> append(A - 1, B) + 1 :|: A >= 1
  append(A,B) -> B :|: A == 0
  split(A) -> split(A - 1) + 1 :|: A >= 1
  split(A) -> 0 :|: A == 0
#  qs(A) -> append(qs(split(E)), qs(split(F))) :|: A >= 1 && A == E + F
  qs(A) -> append(qs(split(A-1)), qs(split(1))) :|: A >= 1
  qs(A) -> 0 :|: A == 0
  start(A) -> qs(A)
)
