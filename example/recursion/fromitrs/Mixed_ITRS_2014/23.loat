(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y z)
(RULES
eval_1((x), (y), (z)) -> eval_2((x), (y), (z)) :|: y>=0 && z>=0 && x>=0 && x>z
eval_2((x), (y), (z)) -> eval_2((x), (-1+y), (z)) :|: y>=0 && z>=0 && x>=0 && x>z && y>z
eval_2((x), (y), (z)) -> eval_1((-1+x), (y), (z)) :|: y>=0 && z>=0 && x>=0 && x>z && z>=y
)
