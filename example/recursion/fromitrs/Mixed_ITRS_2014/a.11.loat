(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y z)
(RULES
eval_1((x), (y), (z)) -> eval_2((x), (y), (z)) :|: z>=0 && x>=0 && y>=0 && x>y
eval_2((x), (y), (z)) -> eval_1((x), (1+y), (z)) :|: z>=0 && x>=0 && y>=0 && x>z
eval_2((x), (y), (z)) -> eval_1((x), (y), (1+z)) :|: z>=0 && x>=0 && y>=0 && x>z
eval_2((x), (y), (z)) -> eval_1((-1+x), (y), (z)) :|: z>=0 && x>=0 && y>=0 && z>=x
)
