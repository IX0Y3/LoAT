(GOAL COMPLEXITY)
(VAR x y x_0 x_1)
(RULES
random((x)) -> rand((x), (1)) :|: x>=0 && x>=0
rand((x), (y)) -> y :|: y>=0 && x>=0 && x==0
rand((x), (y)) -> rand((-1+x), id_inc((y))) :|: y>=0 && x>=0 && x>0
id_inc((x_0)) -> 1+x :|: x>=0 && x_0==1+x && x_0>0
id_inc((x_1)) -> 2+x :|: x>=0 && x_1==1+x && x_1>0
)
