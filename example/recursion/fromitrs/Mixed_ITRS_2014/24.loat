(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_1))
(VAR x y)
(RULES
eval_1((x), (y)) -> eval_2((x), (y)) :|: x>=0 && y>=0 && x>0 && y>0 && x>y
eval_1((x), (y)) -> eval_3((x), (y)) :|: x>=0 && y>=0 && x>0 && y>0 && y>=x
eval_2((x), (y)) -> eval_2((-1+x), (y)) :|: x>=0 && y>=0 && x>0
eval_2((x), (y)) -> eval_1((x), (y)) :|: x>=0 && y>=0 && 0>=x
eval_3((x), (y)) -> eval_3((x), (-1+y)) :|: x>=0 && y>=0 && y>0
eval_3((x), (y)) -> eval_1((x), (y)) :|: x>=0 && y>=0 && 0>=y
)
