(GOAL COMPLEXITY)
(VAR x y z)
(RULES
eval((x), (y), (z)) -> eval((-1+x), (y), (z)) :|: z>=0 && x>=0 && y>=0 && x+y>z && z>=0 && x>0
eval((x), (y), (z)) -> eval((x), (-1+y), (z)) :|: z>=0 && x>=0 && y>=0 && x+y>z && z>=0 && 0>=x && y>0
eval((x), (y), (z)) -> eval((x), (y), (z)) :|: z>=0 && x>=0 && y>=0 && x+y>z && z>=0 && 0>=x && 0>=y
)
