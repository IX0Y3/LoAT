(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval))
(VAR x y)
(RULES
eval((x), (y)) -> eval((-1+x), (y)) :|: x>=0 && y>=0 && x>0
eval((x), (y)) -> eval((-1+x), (y)) :|: x>=0 && y>=0 && y>0 && x>0
eval((x), (y)) -> eval((x), (-1+y)) :|: x>=0 && y>=0 && x>0 && 0>=x && y>0
eval((x), (y)) -> eval((x), (-1+y)) :|: x>=0 && y>=0 && y>0 && 0>=x
eval((x), (y)) -> eval((x), (y)) :|: x>=0 && y>=0 && x>0 && 0>=x && 0>=y
eval((x), (y)) -> eval((x), (y)) :|: x>=0 && y>=0 && y>0 && 0>=x && 0>=y
)
