(GOAL COMPLEXITY)
(VAR X x x_0 x_1 x_2)
(RULES
f((x)) -> f(((1) + f((1)))) :|: x==2
f((X)) -> 1+X :|: X>=0
a -> 1
g((X)) -> 1+X :|: X>=0
activate((x_0)) -> f((X)) :|: X>=0 && x_0==1+X && x_0>0
activate((x_1)) -> a :|: x_1==1
activate((x_2)) -> g((X)) :|: X>=0 && x_2==1+X && x_2>0
activate((X)) -> X :|: X>=0
)
