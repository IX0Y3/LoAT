(GOAL COMPLEXITY)
(VAR X X1 X2 XS x x_0 x_1 x_2 x_3)
(RULES
a__zeros -> 2
a__tail((x)) -> mark((XS)) :|: XS>=0 && X>=0 && x==1+XS+X && x>0
mark((x_0)) -> a__zeros :|: x_0==1
mark((x_1)) -> a__tail(mark((X))) :|: X>=0 && x_1==1+X && x_1>0
mark((x_2)) -> ((1) + mark((X1))) + (X2) :|: X1>=0 && X2>=0 && x_2==1+X1+X2 && x_2>0
mark((x_3)) -> 0 :|: x_3==0
a__zeros -> 1
a__tail((X)) -> 1+X :|: X>=0
)
