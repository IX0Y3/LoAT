(GOAL COMPLEXITY)
(VAR X x x_0 x_1 x_2 x_3)
(RULES
a__g((X)) -> a__h((X)) :|: X>=0
a__c -> 1
a__h((x)) -> a__g((1)) :|: x==1
mark((x_0)) -> a__g((X)) :|: X>=0 && x_0==1+X && x_0>0
mark((x_1)) -> a__h((X)) :|: X>=0 && x_1==1+X && x_1>0
mark((x_2)) -> a__c :|: x_2==1
mark((x_3)) -> 1 :|: x_3==1
a__g((X)) -> 1+X :|: X>=0
a__h((X)) -> 1+X :|: X>=0
a__c -> 1
)
