(GOAL COMPLEXITY)
(VAR X Y Z x x_0 x_1 x_2 x_3 x_4)
(RULES
sel((x), (x_0)) -> sel((X), (Z)) :|: X>=0 && Y>=0 && Z>=0 && x==1+X && x>0 && x_0==1+Y && x_0>0
f((X)) -> 1+X :|: X>=0
g((x_1)) -> 1 :|: x_1==0
g((x_2)) -> (1) + ((1) + g((X))) :|: X>=0 && x_2==1+X && x_2>0
sel((x_3), (x_4)) -> X :|: X>=0 && x_3==0 && x_4==1+X && x_4>0
)
