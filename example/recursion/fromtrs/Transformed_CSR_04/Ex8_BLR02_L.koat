(GOAL COMPLEXITY)
(VAR N X XS Y x x_0 x_1 x_2 x_3 x_4)
(RULES
sel((x), (x_0)) -> sel((N), (XS)) :|: X>=0 && XS>=0 && N>=0 && x==1+N && x>0 && x_0==1+X && x_0>0
fib((N)) -> sel((N), fib1((1), (1))) :|: N>=0
fib1((X), (Y)) -> 1+X :|: X>=0 && Y>=0
add((x_1), (X)) -> X :|: X>=0 && x_1==0
add((x_2), (Y)) -> (1) + add((X), (Y)) :|: X>=0 && Y>=0 && x_2==1+X && x_2>0
sel((x_3), (x_4)) -> X :|: X>=0 && x_3==0 && x_4==1+X && x_4>0
)
