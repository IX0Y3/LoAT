(GOAL COMPLEXITY)
(VAR X Y x x_0 x_1)
(RULES
f((X)) -> if((X), (1), (2)) :|: X>=0
if((x), (X), (Y)) -> X :|: X>=0 && Y>=0 && x==1
if((x_0), (X), (Y)) -> activate((Y)) :|: X>=0 && Y>=0 && x_0==0
f((X)) -> 1+X :|: X>=0
activate((x_1)) -> f((X)) :|: X>=0 && x_1==1+X && x_1>0
activate((X)) -> X :|: X>=0
)
