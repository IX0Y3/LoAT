(GOAL COMPLEXITY)
(VAR X x x_0 x_1 x_2 x_3 x_4)
(RULES
f((x)) -> 4 :|: x==0
f((x_0)) -> f(p((1))) :|: x_0==1
p((x_1)) -> 0 :|: x_1==1
f((X)) -> 1+X :|: X>=0
activate((x_2)) -> f(activate((X))) :|: X>=0 && x_2==1+X && x_2>0
activate((x_3)) -> (1) + activate((X)) :|: X>=0 && x_3==1+X && x_3>0
activate((x_4)) -> 0 :|: x_4==1
activate((X)) -> X :|: X>=0
)
