(GOAL COMPLEXITY)
(VAR X X1 X2 X3 Y x x_0 x_1 x_2 x_3 x_4)
(RULES
a__f((X), (x), (Y)) -> a__f((Y), (Y), (Y)) :|: X>=0 && Y>=0 && x==1+X && x>0
a__g((x_0)) -> 1 :|: x_0==1
a__b -> 1
mark((x_1)) -> a__f((X1), (X2), (X3)) :|: X3>=0 && X1>=0 && X2>=0 && x_1==1+X3+X1+X2 && x_1>0
mark((x_2)) -> a__g(mark((X))) :|: X>=0 && x_2==1+X && x_2>0
mark((x_3)) -> a__b :|: x_3==1
mark((x_4)) -> 1 :|: x_4==1
a__f((X1), (X2), (X3)) -> 1+X3+X1+X2 :|: X3>=0 && X1>=0 && X2>=0
a__g((X)) -> 1+X :|: X>=0
a__b -> 1
)
