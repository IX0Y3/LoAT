(GOAL COMPLEXITY)
(VAR X X1 X2 X3 x x_0 x_1 x_2)
(RULES
a__f((x), (X), (X)) -> a__f((X), a__b, (1)) :|: X>=0 && x==1
a__b -> 1
mark((x_0)) -> a__f((X1), mark((X2)), (X3)) :|: X2>=0 && X3>=0 && X1>=0 && x_0==1+X2+X3+X1 && x_0>0
mark((x_1)) -> a__b :|: x_1==1
mark((x_2)) -> 1 :|: x_2==1
a__f((X1), (X2), (X3)) -> 1+X2+X3+X1 :|: X2>=0 && X3>=0 && X1>=0
a__b -> 1
)
