(GOAL COMPLEXITY)
(VAR X Y Z x x_0 x_1 x_2 x_3)
(RULES
from((X)) -> 3+2*X :|: X>=0
sel((x), (x_0)) -> X :|: X>=0 && Y>=0 && x==0 && x_0==1+X+Y && x_0>0
sel((x_1), (x_2)) -> sel((X), activate((Z))) :|: X>=0 && Y>=0 && Z>=0 && x_1==1+X && x_1>0 && x_2==1+Y+Z && x_2>0
from((X)) -> 1+X :|: X>=0
activate((x_3)) -> from((X)) :|: X>=0 && x_3==1+X && x_3>0
activate((X)) -> X :|: X>=0
)
