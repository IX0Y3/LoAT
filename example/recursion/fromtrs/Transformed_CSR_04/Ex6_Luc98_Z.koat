(GOAL COMPLEXITY)
(VAR X X1 X2 Y Z x x_0 x_1 x_2 x_3)
(RULES
first((x), (X)) -> 1 :|: X>=0 && x==0
first((x_0), (x_1)) -> (1+Y) + ((1+X) + activate((Z))) :|: Y>=0 && X>=0 && Z>=0 && x_0==1+X && x_0>0 && x_1==1+Y+Z && x_1>0
from((X)) -> 3+2*X :|: X>=0
first((X1), (X2)) -> 1+X2+X1 :|: X2>=0 && X1>=0
from((X)) -> 1+X :|: X>=0
activate((x_2)) -> first((X1), (X2)) :|: X2>=0 && X1>=0 && x_2==1+X2+X1 && x_2>0
activate((x_3)) -> from((X)) :|: X>=0 && x_3==1+X && x_3>0
activate((X)) -> X :|: X>=0
)
