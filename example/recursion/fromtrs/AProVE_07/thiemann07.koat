isZero((x_6)) -> 0 :|: x>=0 && x_6==1+x && x_6>0
plus((x), (y)) -> ifplus(isZero((x)), (x), (y)) :|: x>=0 && y>=0
ifplus((x_7), (x), (y)) -> y :|: x>=0 && y>=0 && x_7==1
ifplus((x_8), (x), (y)) -> (1) + plus(p((x)), (y)) :|: x>=0 && y>=0 && x_8==0
times((x), (y)) -> iftimes(isZero((x)), (x), (y)) :|: x>=0 && y>=0
iftimes((x_9), (x), (y)) -> 0 :|: x>=0 && y>=0 && x_9==1
iftimes((x_10), (x), (y)) -> plus((y), times(p((x)), (y))) :|: x>=0 && y>=0 && x_10==0
