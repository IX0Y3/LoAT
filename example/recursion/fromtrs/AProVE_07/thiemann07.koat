prod((l)) -> if(shorter((l), (0)), shorter((l), (1)), (l)) :|: l>=0
if((x_18), (b), (l)) -> 1 :|: l>=0 && b>=0 && x_18==1
if((x_19), (b), (l)) -> if2((b), (l)) :|: l>=0 && b>=0 && x_19==0
if2((x_20), (l)) -> car((l)) :|: l>=0 && x_20==1
if2((x_21), (l)) -> prod((((1) + times(car((l)), cadr((l)))) + cddr((l)))) :|: l>=0 && x_21==0
)
