(GOAL COMPLEXITY)
(VAR X Y x x_0 x_1 x_2)
(RULES
iwasplus((X), (x)) -> X :|: X>=0 && x==0
iwasplus((X), (x_0)) -> (1) + iwasplus((X), (Y)) :|: Y>=0 && X>=0 && x_0==1+Y && x_0>0
double((X)) -> iwasplus((X), (X)) :|: X>=0
f((x_1), (x_2), (X)) -> f((X), double((X)), (X)) :|: X>=0 && x_1==0 && x_2==1
g((X), (Y)) -> X :|: Y>=0 && X>=0
g((X), (Y)) -> Y :|: Y>=0 && X>=0
)
