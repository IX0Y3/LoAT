(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3)
(RULES
f((x), (x_0), (x_1)) -> f((y), (y), f((y), (x), (y))) :|: y>=0 && x>=0 && x_0==1+x && x_0>0 && x_1==1+y && x_1>0
f((x_2), (y), (z)) -> f((x), (2+y), (1+z)) :|: y>=0 && z>=0 && x>=0 && x_2==1+x && x_2>0
f((x_3), (x), (y)) -> 1+y :|: y>=0 && x>=0 && x_3==1+x && x_3>0
g((x), (y)) -> x :|: y>=0 && x>=0
g((x), (y)) -> y :|: y>=0 && x>=0
)
