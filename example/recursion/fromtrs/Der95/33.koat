(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3 x_4)
(RULES
h((z), (x_0)) -> h((1+z), d((z), (x))) :|: x>=0 && z>=0 && x_0==1+x && x_0>0
d((z), (x_1)) -> 1 :|: z>=0 && x_1==1
d((z), (x_2)) -> (2+x) + d((z), (y)) :|: x>=0 && y>=0 && z>=0 && x_2==1+x+y && x_2>0
d((x_3), (x_4)) -> ((1) + d((1+z), (1+x+y))) + d((z), (1+x+y)) :|: x>=0 && y>=0 && z>=0 && x_3==1+z && x_3>0 && x_4==2+x+y && x_4>0
)
