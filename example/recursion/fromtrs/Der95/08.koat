(GOAL COMPLEXITY)
(VAR x y x_0 x_1 x_2 x_3 x_4)
(RULES
D((x_0)) -> 1 :|: x_0==1
D((x_1)) -> 0 :|: x_1==1
D((x_2)) -> ((1) + D((x))) + D((y)) :|: x>=0 && y>=0 && x_2==1+x+y && x_2>0
D((x_3)) -> ((1) + ((1+y) + D((x)))) + ((1+x) + D((y))) :|: x>=0 && y>=0 && x_3==1+x+y && x_3>0
D((x_4)) -> ((1) + D((x))) + D((y)) :|: x>=0 && y>=0 && x_4==1+x+y && x_4>0
)
