(GOAL COMPLEXITY)
(VAR x x_0 x_1 x_2 x_3 x_4)
(RULES
top((x_0)) -> top(check((1+x))) :|: x>=0 && x_0==1+x && x_0>0
check((x_1)) -> (1) + check((x)) :|: x>=0 && x_1==1+x && x_1>0
check((x_2)) -> (1) + check((x)) :|: x>=0 && x_2==1+x && x_2>0
check((x_3)) -> (1) + check((x)) :|: x>=0 && x_3==1+x && x_3>0
check((x_4)) -> 1+x :|: x>=0 && x_4==1+x && x_4>0
)
