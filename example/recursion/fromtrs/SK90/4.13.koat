(GOAL COMPLEXITY)
(VAR x y x_0 x_1 x_2 x_3 x_4)
(RULES
iwasminus((x_0), (y)) -> 0 :|: y>=0 && x_0==0
iwasminus((x), (x_1)) -> x :|: x>=0 && x_1==0
iwasminus((x), (x_2)) -> ((3+x+y) + ((1) + iwasminus((x), p((1+y))))) + (0) :|: x>=0 && y>=0 && x_2==1+y && x_2>0
p((x_3)) -> 0 :|: x_3==0
p((x_4)) -> x :|: x>=0 && x_4==1+x && x_4>0
)
