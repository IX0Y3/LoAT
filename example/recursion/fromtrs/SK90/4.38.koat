(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2)
(RULES
g((x_0), (z)) -> (1+x) + g((y), (z)) :|: y>=0 && z>=0 && x>=0 && x_0==1+y+x && x_0>0
g((x_1), (z)) -> g((x), (1+y+z)) :|: y>=0 && z>=0 && x>=0 && x_1==1+y+x && x_1>0
g((x), (x_2)) -> ((1) + g((x), (y))) + (z) :|: y>=0 && z>=0 && x>=0 && x_2==1+y+z && x_2>0
)
