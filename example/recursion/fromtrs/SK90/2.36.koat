(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3)
(RULES
implies((x_0), (y)) -> 1+x+y :|: x>=0 && y>=0 && x_0==1+x && x_0>0
implies((x_1), (x_2)) -> implies((y), (1+x+z)) :|: x>=0 && y>=0 && z>=0 && x_1==1+x && x_1>0 && x_2==1+y+z && x_2>0
implies((x), (x_3)) -> (1+y) + implies((x), (z)) :|: x>=0 && y>=0 && z>=0 && x_3==1+y+z && x_3>0
)
