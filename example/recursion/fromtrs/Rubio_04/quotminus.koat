(GOAL COMPLEXITY)
(VAR X Y x x_0 x_1 x_2 x_3 x_4)
(RULES
plus((x), (Y)) -> Y :|: Y>=0 && x==0
plus((x_0), (Y)) -> (1) + plus((X), (Y)) :|: X>=0 && Y>=0 && x_0==1+X && x_0>0
quot((x_1), (x_2)) -> 0 :|: Y>=0 && x_1==0 && x_2==1+Y && x_2>0
quot((x_3), (x_4)) -> (1) + quot((1+X+Y), (1+Y)) :|: X>=0 && Y>=0 && x_3==1+X && x_3>0 && x_4==1+Y && x_4>0
)
