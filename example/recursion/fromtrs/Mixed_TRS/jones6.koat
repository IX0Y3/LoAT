(GOAL COMPLEXITY)
(VAR a d k x x_0 x_1 x_2 x_3)
(RULES
f((a), (x_0)) -> g((a), (1)) :|: a>=0 && x_0==1
f((a), (x_1)) -> f((1+x+a), (k)) :|: k>=0 && x>=0 && a>=0 && x_1==1+k+x && x_1>0
g((x_2), (d)) -> d :|: d>=0 && x_2==1
g((x_3), (d)) -> g((k), (1+d+x)) :|: d>=0 && k>=0 && x>=0 && x_3==1+k+x && x_3>0
)
