(GOAL COMPLEXITY)
(VAR x y x_0 x_1 x_2 x_3)
(RULES
f((x), (x_0)) -> f((2+x), (y)) :|: x>=0 && y>=0 && x_0==2+y && x_0>0
f((x), (x_1)) -> f((2+x), (y)) :|: x>=0 && y>=0 && x_1==2+y && x_1>0
f((x_2), (y)) -> f((x), (1+y)) :|: x>=0 && y>=0 && x_2==1+x && x_2>0
f((x_3), (y)) -> f((x), (1+y)) :|: x>=0 && y>=0 && x_3==1+x && x_3>0
)
