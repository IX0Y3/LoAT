(GOAL COMPLEXITY)
(VAR x y x_0 x_1 x_2 x_3)
(RULES
f((x), (x_0)) -> f((4+x), (y)) :|: y>=0 && x>=0 && x_0==3+y && x_0>0
f((x_1), (y)) -> f((x), (1+y)) :|: y>=0 && x>=0 && x_1==1+x && x_1>0
f((x_2), (y)) -> f((x), (1+y)) :|: y>=0 && x>=0 && x_2==1+x && x_2>0
f((x_3), (y)) -> f((x), (1+y)) :|: y>=0 && x>=0 && x_3==1+x && x_3>0
)
