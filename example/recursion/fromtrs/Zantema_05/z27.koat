(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3 x_4)
(RULES
f((x_0), (x_1), (x)) -> f((1+x), (1+x), (x)) :|: x>=0 && x_0==0 && x_1==1
f((x_2), (y), (z)) -> (1) + f((x), (y), (z)) :|: y>=0 && z>=0 && x>=0 && x_2==1+x && x_2>0
f((x), (x_3), (z)) -> (1) + f((x), (y), (z)) :|: y>=0 && z>=0 && x>=0 && x_3==1+y && x_3>0
f((x), (y), (x_4)) -> (1) + f((x), (y), (z)) :|: y>=0 && z>=0 && x>=0 && x_4==1+z && x_4>0
)
