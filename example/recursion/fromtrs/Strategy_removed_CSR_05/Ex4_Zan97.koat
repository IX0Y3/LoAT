(GOAL COMPLEXITY)
(VAR X Y Z x x_0 x_1 x_2 x_3 x_4)
(RULES
f((X)) -> (1+X) + f(g((X))) :|: X>=0
g((x)) -> 1 :|: x==0
g((x_0)) -> (1) + ((1) + g((X))) :|: X>=0 && x_0==1+X && x_0>0
sel((x_1), (x_2)) -> X :|: X>=0 && Y>=0 && x_1==0 && x_2==1+X+Y && x_2>0
sel((x_3), (x_4)) -> sel((X), (Z)) :|: X>=0 && Y>=0 && Z>=0 && x_3==1+X && x_3>0 && x_4==1+Y+Z && x_4>0
)
