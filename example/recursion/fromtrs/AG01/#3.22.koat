(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2)
(RULES
times((x), (x_0)) -> ((1) + times((x), ((1+y) + times((1+z), (0))))) + times((x), (1+z)) :|: z>=0 && x>=0 && y>=0 && x_0==2+z+y && x_0>0
times((x), (x_1)) -> 0 :|: x>=0 && x_1==0
times((x), (x_2)) -> ((1) + times((x), (y))) + (x) :|: x>=0 && y>=0 && x_2==1+y && x_2>0
)
