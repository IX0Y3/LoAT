(GOAL COMPLEXITY)
(VAR k l x y x_0 x_1 x_2 x_3 x_4)
(RULES
sum((x_0)) -> 2+x :|: x>=0 && x_0==2+x && x_0>0
sum((x_1)) -> sum((((1) + plus((x), (y))) + (l))) :|: l>=0 && x>=0 && y>=0 && x_1==2+l+x+y && x_1>0
sum((x_2)) -> sum(((1+l) + sum((2+x+y+k)))) :|: l>=0 && x>=0 && y>=0 && k>=0 && x_2==3+l+x+y+k && x_2>0
plus((x_3), (y)) -> y :|: y>=0 && x_3==0
plus((x_4), (y)) -> (1) + plus((x), (y)) :|: x>=0 && y>=0 && x_4==1+x && x_4>0
)
