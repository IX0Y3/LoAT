(GOAL COMPLEXITY)
(VAR x y x_0 x_1 x_2 x_3)
(RULES
f((x_0)) -> f((2+y+x)) :|: y>=0 && x>=0 && x_0==2+y+x && x_0>0
f((x_1)) -> g((1+y+x)) :|: y>=0 && x>=0 && x_1==3+y+x && x_1>0
g((x_2)) -> g((2+y+x)) :|: y>=0 && x>=0 && x_2==2+y+x && x_2>0
g((x_3)) -> f((1+y+x)) :|: y>=0 && x>=0 && x_3==3+y+x && x_3>0
)
