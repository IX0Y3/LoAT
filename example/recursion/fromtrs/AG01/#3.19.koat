(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3 x_4)
(RULES
minus((x), (x_0)) -> x :|: x>=0 && x_0==0
minus((x_1), (x_2)) -> minus((x), (y)) :|: x>=0 && y>=0 && x_1==1+x && x_1>0 && x_2==1+y && x_2>0
double((x_3)) -> 0 :|: x_3==0
double((x_4)) -> (1) + ((1) + double((x))) :|: x>=0 && x_4==1+x && x_4>0
)
