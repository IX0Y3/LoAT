(GOAL COMPLEXITY)
(VAR x y z x_0 x_1 x_2 x_3)
(RULES
max((x_0)) -> x :|: x>=0 && x_0==1+x && x_0>0
max((x_1)) -> y :|: y>=0 && x_1==3+y && x_1>0
max((x_2)) -> (1) + max((3+x+y)) :|: x>=0 && y>=0 && x_2==5+x+y && x_2>0
max((x_3)) -> max(((2+x) + ((1) + max((1+y+z))))) :|: x>=0 && y>=0 && z>=0 && x_3==3+x+y+z && x_3>0
)
