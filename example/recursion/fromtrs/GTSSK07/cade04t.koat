(GOAL COMPLEXITY)
(VAR u v x y x_0 x_1 x_2 x_3)
(RULES
minus((x), (y)) -> cond(min((x), (y)), (x), (y)) :|: x>=0 && y>=0
cond((y), (x), (y)) -> (1) + minus((x), (1+y)) :|: x>=0 && y>=0
min((x_0), (v)) -> 0 :|: v>=0 && x_0==0
min((u), (x_1)) -> 0 :|: u>=0 && x_1==0
min((x_2), (x_3)) -> (1) + min((u), (v)) :|: u>=0 && v>=0 && x_2==1+u && x_2>0 && x_3==1+v && x_3>0
)
