(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS start))
(VAR x y z)
(RULES
  start(x,y) -> h(f(x,y)) :|: y < 0
  f(x,y) -> f(x-1,y+x)+x :|: x > 0
  f(x,y) -> y :|: x = 0
  g(x,y) -> g(x,y-1)+1 :|: y > 0
  g(x,y) -> x :|: y = 0
  h(x) -> g(h(z) + 1, h(x-z-1)) :|: x > 0 && z >= 0 && z <= x - 1
  h(x) -> 0 :|: x = 0
)
