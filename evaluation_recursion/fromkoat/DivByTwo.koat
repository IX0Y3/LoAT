(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS divByTwo))
(VAR A B)
(RULES
  divByTwo(A) -> Com_1(m2(A)) :|: A >= 1
  m0(A) -> Com_1(m1(A))
  m5(A) -> foo(m3(A),m0(A)) :|: A >= 1
  m2(A) -> Com_1(m5(A)) :|: A >= 1
  m6(A) -> Com_1(m0(B)) :|: A >= 2*B && A >= 1
  m1(A) -> Com_1(m6(A))
  m1(A) -> Com_1(m4(A))
  foo(A,B) -> A + B
  m3(A) -> 0
  m4(A) -> 0
  m6(A) -> 0
)
